<Window x:Class="fibonacciSeries.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fibonacciSeries"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="lblHeader" Content="Input Fibonacci max number" HorizontalAlignment="Left" Margin="63,48,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbxMaxNumber" HorizontalAlignment="Left" Height="23" Margin="239,48,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" TextChanged="textBox_TextChanged"/>
        <Button x:Name="btnGenerate" Content="Generate" HorizontalAlignment="Left" Margin="279,76,0,0" VerticalAlignment="Top" Width="75" Click="btnGenerate_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="427,276,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.232,0.609"/>
        <TextBox x:Name="tbxResult" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="415" Margin="63,142,0,0" TextChanged="textBox_TextChanged"/>

    </Grid>
</Window>
